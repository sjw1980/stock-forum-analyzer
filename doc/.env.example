# 데이터베이스 설정 (개발/테스트용)
DB_HOST=localhost
DB_PORT=3306
DB_USER=crawler
DB_PASSWORD=your_password_here
DB_NAME=stock_crawling
DB_CHARSET=utf8mb4

# MySQL Root 비밀번호 (Docker 컨테이너용)
MYSQL_ROOT_PASSWORD=your_root_password_here

# 크롤링 설정
CRAWLING_DELAY=1.0
MAX_PAGES=10
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=crawler.log
